{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "PUT", "PATCH", "DELETE", "POST", "OPTIONS`"]
    }
  ],
  "build": {
    "env": {
      "DATABASE_URL": "mysql://root:123123@localhost:3306/tani_direct"
    }
  }

}
